from tkinter import *
std=Tk()
std.title("***student application***");
Label(std,text="student id:").grid(row=0,column=0)
Label(std,text="student name:").grid(row=2,column=0)
Label(std,text="father name:").grid(row=4,column=0)
Label(std,text="group :").grid(row=6,column=0)
Label(std,text="phone number:").grid(row=8,column=0)
Label(std,text="email id:").grid(row=10,column=0)
Label(std,text="address:").grid(row=12,column=0)
Entry(std).grid(row=0,column=1)
Entry(std).grid(row=2,column=1)
Entry(std).grid(row=4,column=1)
Entry(std).grid(row=6,column=1)
Entry(std).grid(row=8,column=1)
Entry(std).grid(row=10,column=1)
Entry(std).grid(row=12,column=1)
Button(std,text="submit").grid(row=15,column=1)
Button(std,text="cancel").grid(row=15,column=8)
